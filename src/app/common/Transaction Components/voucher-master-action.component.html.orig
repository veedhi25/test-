<div class="print-modal-sticky" *ngIf="promptPrintDevice" style="display : block; padding: 0px;  margin-bottom: 25px;height: auto;
  position: absolute;right: 65%;top: 5px;width: 50%;z-index: 9999999">

  <div class="modal-content modal-content-main-area">
    <div class="modal-header">
      <h4 class="modal-title" id="myModalLabel">Available Printing Device Supported</h4>
    </div>
    <div class="modal-body">
      <ng-container *ngIf="formWisePrintProfile && (!formWisePrintProfile.length)">
        <form>
          <ng-container
            *ngIf="(_trnMainService.TrnMainObj.VoucherType == 14 || _trnMainService.TrnMainObj.VoucherType == 15 || _trnMainService.TrnMainObj.VoucherType == 20) && ( _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='retailer' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ak' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ck' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='gak' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='pms')">
            <input type="radio" [formControl]="printControl" name="printOption" value=0>[ALT+1]POS Printer [3mm]
            <input style=" width: 50px;" type="number" [(ngModel)]="_trnMainService.posPrintCount "
              name="posPrintCount">
            <br>
            <input type="radio"  [formControl]="printControl"  name="printOption"
               value=5>POS Printer without GST [3mm]<br>

            <input type="radio"  [formControl]="printControl"  name="printOption"  value=6>POS Printer [2mm]<br>
          </ng-container>
          <input type="radio"  [formControl]="printControl"  name="printOption"  value=58>POS Printer Matrix [3mm]<br>

          <input type="radio" [formControl]="printControl" name="printOption" value=1 checked>[ALT+2]Laser
          printer[A4]<br>
          <ng-container
            *ngIf="_trnMainService.TrnMainObj.VoucherType == 14 &&(_trnMainService.userProfile.CompanyInfo.ORG_TYPE=='superstockist' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='distributor'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='superdistributor' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='retailer' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ak'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ck'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='pms' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='gak')">
            <input type="radio" [formControl]="printControl" name="printOption" value=2 checked>[ALT+3]A5 print<br>
          </ng-container>
          <ng-container
            *ngIf="_trnMainService.TrnMainObj.VoucherType == 14 &&(_trnMainService.userProfile.CompanyInfo.ORG_TYPE=='superstockist' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='distributor'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='superdistributor' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='retailer' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ak'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='ck'|| _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='pms' || _trnMainService.userProfile.CompanyInfo.ORG_TYPE=='gak')">
            <input type="radio" [formControl]="printControl" name="printOption" value=3 checked>[ALT+3]A5 print(NON
            GST)<br>
          </ng-container>

          <input type="radio" [formControl]="printControl" name="printOption" value=10>Customized KW print<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=11>Customized ITC print<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=21>Customized GT print<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=12>Download Excel File<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=13>A4 Ashoka Wholesale<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=14>A5 Ashoka Wholesale<br>
          <input type="radio" [formControl]="printControl" name="printOption" value=20>3 MM Html WITHOUT GST<br>

        </form>
      </ng-container>



      <ng-container *ngIf="formWisePrintProfile && formWisePrintProfile.length">
        <ng-container *ngFor="let profile of formWisePrintProfile">
          <input type="radio" [formControl]="printControl" name="printOption"
            value="{{profile.controlValue}}">{{profile?.profileNameLabel}}<br>
        </ng-container>

      </ng-container>



    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-default" data-dismiss="modal" (click)="cancelprint()">[Esc]Cancel</button>
      <!-- setPrinterAndprint() -->
      <button type="button" class="btn btn-primary" (click)="sendToPrint()">[ALT+P]Print</button>
    </div>

  </div>
</div>
