            <option *ngFor="let emp of employeeList" value="{{emp.ACID}}">{{emp.ACNAME}}</option>
          </select>
        </div>
        <div class="form-group">
          <label style="width: 100% !important;">Employee Mobile</label>
          <input type="text" name="emobile" [(ngModel)]="_trnMainService.TrnMainObj.BILLTOMOB" class="form-control"
            (keydown)="$event.preventDefault()">

        </div>
        <div class="form-group">
          <label style="width: 100% !important;">Employee Email</label>

          <input type="text" name="emobile" [(ngModel)]="_trnMainService.TrnMainObj.BILLTOTEL" class="form-control"
            (keydown)="$event.preventDefault()">


        </div>
        <div class="form-group">
          <label style="width: 100% im !important;">Return Date</label>

          <input type="date" id="retdate" name="retdate" class="form-control"
            [(ngModel)]="_trnMainService.TrnMainObj.BSDATE">
        </div>
      </div>
    </div>

  </div>
</div>

<scheme-offer [activeIndex]="activerowIndex" [showScheme]="_trnMainService.showSchemeOffer"
  (onSchemeApplyEmitted)="onSchemeApplyEmitted($event)">

</scheme-offer>


<div class="modal-sticky" *ngIf="showAdvancePopUpForSalesOrder" style="display : block; padding: 0px;  margin-bottom: 25px;height: 210px;
  position: absolute;right: 50%;top: 5px;width: 550px;z-index: 9999999">
  <div class="modal-content modal-content-main-area" style="width: 100%;">
    <div class="modal-header">
      <h4 class="modal-title" id="myModalLabel">Please provide advance amount if any and press Ok.</h4>
    </div>

    <div class="row" style="margin: 30px;">
      <label style="width: 30%;">Advance Amount :</label>
      <input type="number" style="width:40%;" [(ngModel)]="_trnMainService.TrnMainObj.ADVANCE" />
    </div>

    <div class="modal-footer">

      <button type="button" style="margin-top: 5px;" class="btn btn-primary"
        (click)="OkAdvanceFromSalesOrder()">OK</button>
    </div>


  </div>

</div>