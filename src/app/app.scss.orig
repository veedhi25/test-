.notification-sticky {
  position : fixed;
  top      : 1vh;
  right    : 10vw;
  width    : 28px;
  height   : 30px;
  z-index  : 9999999999;
  font-size: 20px;
}


.notification {
  // background-color: #555;
  color          : #616363 !important;
  text-decoration: none;
  padding        : 7px 8px;
  border-radius  : 50%;
  cursor         : pointer;
}


.notification .badge {
  position     : relative;
  bottom       : 30px;
  left         : 5px;
  padding      : 4px 4px;
  border-radius: 50%;
  background   : red;
  font-size    : 7px;
  font-weight  : bold;
  color        : white;
}

.notification-card {
  box-shadow: 0 1px 0px 0 rgba(0, 0, 0, 0.2);
  transition: 0.3s;
  width     : 100%;
}

.notification-card:hover {
  box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);
}

.notification-card-inside {
  // padding: 8px 8px;
  padding: 5px 5px;
}

button.close {
  margin-right: 8px;
}

.cell-comment {
  display      : none;
  position     : absolute;
  z-index      : 100;
  color        : #ffffff;
  top          : auto;
  background   : #2bade5;
  width        : 240px;
  text-align   : center;
  border-radius: 10px;
  font-size    : 12px;
  font-family  : cursive;
  line-height  : 20px;
  left         : 18%;
  padding      : 6px 4px;
  border       : 1px solid #2bade5;
}

.cell-with-comment:hover span.cell-comment {
  display: block !important;
}

.cell-with-comment:focus-within span.cell-comment {
  display: block !important;
}



