@import "~normalize.css";
@import "theme/theme";
@import "theme/sass/conf/conf";

body {
  font-family: 'Montserrat', sans-serif;
}

.main-container-transaction {
  background-color: #f1f1f1;
  margin-top      : 26px;
  margin-bottom   : 70px;
  position        : fixed;
  top             : 70px;
  max-width       : 100%;
  height          : 100%;
  width           : 100%;
  margin-left     : 10px;
}

.ng2-smart-table-container table.ng2-smart-table {

  th,
  td {
    border        : 1px solid $border-light !important;
    line-height   : 25px !important;
    font-size     : 12px !important;
    vertical-align: middle;
  }

  color: $default-text;

  input {
    line-height: 23px !important;
  }

  tbody {
    tr:hover {
      background: rgba(0, 0, 0, 0.03);
    }
  }

  a.ng2-smart-sort-link {
    font-size  : 14px !important;
    color      : $default-text;
    font-weight: $font-bolder;

    &.sort {
      font-weight: $font-bolder !important;

      &::after {
        border-bottom-color: $default-text !important;
      }
    }
  }

  .ng2-smart-actions {
    width     : 70px;
    text-align: center;

    .actions {
      float     : none;
      text-align: center;
    }
  }

  a.ng2-smart-action {
    font-size: 14px !important;
    color    : $default-text;
    padding  : 0 5px;
    display  : inline-block;

    &.ng2-smart-action-add-add {
      font-size: 25px !important;
    }
  }
}

nav.ng2-smart-pagination-nav {
  display        : flex;
  justify-content: center;
}


.alert-danger {
  background-color: none !important;
  border-color    : none !important;
  color           : #a94442;
}



.main-container {
  background-color: #f1f1f1;
  margin-top      : 45px;
  margin-bottom   : 70px;
  position        : fixed;
  top             : 70px;
  max-width       : 100%;
  height          : 100%;
  overflow-y      : auto;
  overflow-x      : hidden;
  width           : 100%;
  margin-left     : 10px;
}

.sticky-top {
  position  : fixed;
  left      : 0px;
  height    : 35px;
  padding   : 5px;
  top       : 85px;
  background: #e2eaea !important;
  width     : 100%;
  z-index   : 9;
}

.sticky-bottom {
  position  : fixed;
  bottom    : 20px;
  left      : 0px;
  width     : 100%;
  height    : 35px;
  z-index   : 0;
  background: #e2eaea !important;
}

.modal-sticky {
  position: fixed;
  bottom  : 5px;
  right   : 5px;
  width   : 50%;
  height  : 80%;
  z-index : 9999999;
}

.modal-sticky-hide {
  display: none !important;
}

.custom-page-top {
  position: fixed;
  top     : 0;
  width   : 100%;
  z-index : 9999999999;
}

.custom-page-body {
  margin-top    : 60px;
  padding-bottom: 0px;
  max-height    : 70%;
  overflow-y    : scroll;
  overflow-x    : hidden;
  margin-left   : 0px !important;
}


select.form-control:not([size]):not([multiple]) {
  height: auto !important;
}

.notification-modal-sticky {
  position: fixed;
  top     : 35px;
  right   : 1px;
  width   : 50%;
  height  : 94%;
  z-index : 9999999999;
}

// .message {
