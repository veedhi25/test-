.tap-tabbable a.nav-link {
  padding    : 12px 18px !important;
  color      : #333;
  font-weight: 600;
  margin     : 0 !important;
}

.tap-tabbable a.nav-link.active {
  background: #ffffff !important;
  color     : #4e4e4e !important;
  position  : relative;
}

.tap-tabbable .nav.nav-tabs {
  border-top-left-radius : 0;
  border-top-right-radius: 0;
  border-bottom          : 1px solid transparent;
  background-color       : #11cdef;
}

.tap-tabbable .nav-tabs .nav-item {
  float        : left;
  margin-bottom: -1px !important;
  margin       : 0;
  border-right : 1px solid #0dc0e0;
  min-width    : 120px;
  text-align   : center;
}

.tap-tabbable .nav.nav-tabs>li>a:hover {
  border          : none;
  background-color: #11cdef;
}

.tap-tabbable .tabbable {
  display   : inline-block;
  width     : 100%;
  background: #fff;
  padding   : 0px 0px;
  border    : 1px solid #11cdef;
}

.tap-tabbable .tab-content {
  padding   : 15px 15px 5px 15px;
  background: transparent;
  color     : #000000;
  display   : inline-block;
  width     : 100%;
}


.tap-tabbable .form-group label {
  color      : #172b4d;
  font-size  : 14px;
  line-height: 18px;
  margin     : 10px 0 0;
  text-align : right;
  font-weight: 600;
  width      : auto;
}

.General-inofo .col-sm-3 {
  width: 25% !important
}

.tap-tabbable .nav.nav-tabs>li:first-of-type a {
  border-top-left-radius: 0px;
}

.tap-tabbable a.nav-link.active:after {
  height    : 14px;
  width     : 14px;
  border    : 1px solid #11cdef;
  display   : inline-block;
  position  : absolute;
  bottom    : -6px;
  left      : 0;
  right     : 0;
  margin    : auto;
  transform : rotate(45deg);
  background: #11cdef;
}

.tap-tabbable select.form-control:not([size]):not([multiple]) {
  height: auto !important;
}

.tap-tabbable select.form-control {
  padding: 6px 12px;
  height : 40px !important;
